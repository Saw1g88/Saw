[Script]
Save-Final-Rule = type=rule,script-path=https://raw.githubusercontent.com/Saw1g88/Saw/refs/heads/main/Surge/Script/Rule-Storage.js,argument=region=global&quantity={{{Proxy+数量参数}}}&interval={{{Proxy+间隔参数}}},script-update-interval=-1
Save-China-Rule = type=rule,script-path=https://raw.githubusercontent.com/Saw1g88/Saw/refs/heads/main/Surge/Script/Rule-Storage.js,argument=region=china&matched=1&quantity={{{Direct+数量参数}}}&interval={{{Direct+间隔参数}}},script-update-interval=-1
